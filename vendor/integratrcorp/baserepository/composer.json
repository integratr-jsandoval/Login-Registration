{
    "name": "integratrcorp/baserepository",
    "description": "Base Repository Package",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "league/fractal": "^0.17.0",
        "illuminate/support": "^5.6|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Integratrcorp\\Baserepository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Integratrcorp\\Baserepository\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Angelo Zamora",
            "email": "a.zamora@integratrcorp.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Integratrcorp\\Baserepository\\ServiceProvider"
            ]
        }
    }
}
